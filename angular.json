{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ts-etl-ui": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true,
          "flat": true,
          "changeDetection": "OnPush"
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/ts-etl-ui",
      "sourceRoot": "projects/ts-etl-ui/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/ts-etl-ui",
            "index": "projects/ts-etl-ui/src/index.html",
            "browser": "projects/ts-etl-ui/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/ts-etl-ui/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ts-etl-ui/src/public/favicon.ico"
            ],
            "styles": [
              "projects/ts-etl-ui/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1500kB",
                  "maximumError": "2MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/ts-etl-ui/src/environments/environment.ts",
                  "with": "projects/ts-etl-ui/src/environments/environment.development.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "proxyConfig": "projects/ts-etl-ui/src/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "ts-etl-ui:build:production"
            },
            "development": {
              "buildTarget": "ts-etl-ui:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ts-etl-ui/**/*.ts",
              "projects/ts-etl-ui/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ],
    "analytics": "01302b77-9ae6-43d5-8a3b-07ddfdd186f1"
  }
}
