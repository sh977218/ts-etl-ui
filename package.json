{
  "name": "ts-etl-ui",
  "version": "0.0.0",
  "type": "module",
  "main": "server/server.js",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "serve:integration": "ng serve -c integration",
    "start": "node --env-file .prod.env server/server.js",
    "start:ci": "node --env-file .ci.env server/server.js",
    "build": "ng build -c development && node insert-commit-hash.js",
    "build:prod": "ng build -c production && node insert-commit-hash.js",
    "build:ci": "ng build -c ci && node insert-commit-hash.js",
    "watch": "ng build --watch --configuration development",
    "test": "playwright test --project=chromium",
    "test:smoke": "playwright test --project=debug",
    "test:debug": "playwright test --project=debug --debug",
    "prepare": "husky"
  },
  "private": true,
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "dependencies": {
    "@angular/animations": "^17.0.3",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.3",
    "@angular/compiler": "^17.0.3",
    "@angular/core": "^17.0.3",
    "@angular/forms": "^17.0.3",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.3",
    "@angular/platform-browser-dynamic": "^17.0.3",
    "@angular/router": "^17.0.3",
    "express": "^4.19.2",
    "file-saver": "^2.0.5",
    "mongodb": "^6.7.0",
    "node-fetch": "^3.3.2",
    "rxjs": "~7.5.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.0",
    "@angular-devkit/build-angular": "^17.0.1",
    "@angular-eslint/builder": "17.1.1",
    "@angular-eslint/eslint-plugin": "17.1.1",
    "@angular-eslint/eslint-plugin-template": "17.1.1",
    "@angular-eslint/schematics": "17.1.1",
    "@angular-eslint/template-parser": "17.1.1",
    "@angular/cli": "~17.0.1",
    "@angular/compiler-cli": "^17.0.3",
    "@angular/material": "^17.0.1",
    "@playwright/test": "^1.44.0",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "allure-commandline": "^2.29.0",
    "allure-playwright": "^3.0.0-beta.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.54.0",
    "husky": "^9.0.11",
    "lodash": "^4.17.21",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.2"
  }
}
