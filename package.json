{
  "name": "ts-etl-ui",
  "version": "0.0.0",
  "main": "server/server.js",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve ts-etl-ui",
    "serve:prod": "ng serve ts-etl-ui -c prod",
    "serve:render": "ng serve ts-etl-ui -c render",
    "serve:coverage": "ng serve e2e-coverage -c coverage",
    "serve:integration": "ng serve ts-etl-ui -c integration",
    "serve:integration-local": "ng serve ts-etl-ui -c integration-local",
    "start": "npm run start --prefix server",
    "start:coverage": "npm run start:coverage --prefix server",
    "build": "ng build ts-etl-ui -c render && node server/insert-commit-hash.js",
    "build:prod": "ng build ts-etl-ui -c prod && node server/insert-commit-hash.js",
    "build:coverage": "ng build e2e-coverage -c coverage && node server/insert-commit-hash.js",
    "test": "playwright test --project=ts-etl-ui",
    "test:nlm": "playwright test --project=load-request-nlm",
    "test:smoke": "playwright test --project=debug",
    "test:debug": "playwright test --project=debug --debug",
    "test:ui": "playwright test --project=ts-etl-ui --ui",
    "reset-db": "node --env-file server/.coverage.env server/reset-mongo-db.js",
    "coverage-report": "npx nyc report",
    "prepare": "husky"
  },
  "private": true,
  "engines": {
    "node": ">=20 <21",
    "npm": ">=10"
  },
  "dependencies": {
    "@angular/animations": "^18.2.9",
    "@angular/cdk": "^18.2.10",
    "@angular/common": "^18.2.9",
    "@angular/compiler": "^18.2.9",
    "@angular/core": "^18.2.9",
    "@angular/forms": "^18.2.9",
    "@angular/material": "^18.2.10",
    "@angular/material-moment-adapter": "^18.2.10",
    "@angular/platform-browser": "^18.2.9",
    "@angular/platform-browser-dynamic": "^18.2.9",
    "@angular/router": "^18.2.9",
    "coverage-istanbul-loader": "^3.0.5",
    "file-saver": "^2.0.5",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "nyc": "^17.0.0",
    "rxjs": "~7.5.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "^18.2.10",
    "@angular-eslint/builder": "^18.4.0",
    "@angular-eslint/eslint-plugin": "^18.4.0",
    "@angular-eslint/eslint-plugin-template": "^18.4.0",
    "@angular-eslint/schematics": "^18.4.0",
    "@angular-eslint/template-parser": "^18.4.0",
    "@angular/cli": "^18.2.10",
    "@angular/compiler-cli": "^18.2.9",
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.48.1",
    "@types/file-saver": "^2.0.7",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.12",
    "@types/node": "^22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "lodash": "^4.17.21",
    "ngx-cookie-service": "^18.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.4.2"
  },
  "workspaces": [
    "server"
  ]
}
